<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    
    <record id="view_form_inherit" model="ir.ui.view">
        <field name="name">new.tab.inUser.views.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='references']" position="after">
                <page string="Discount Limit" name="discount_limit">
                    <group>
                        <field name="fixed_limit" widget="monetary"/>
                        <field name="percentage_limit" />
                    </group>
                </page>
            </xpath>
        </field>
    </record>
    
</odoo>